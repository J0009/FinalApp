<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MKX Characters Index App</name>
    <description>Final App
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        J
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/android/icon-36-ldpi.png" platform="android" qualifier="ldpi" />
<icon src="res/android/icon-48-mdpi.png" platform="android" qualifier="mdpi" />
<icon src="res/android/icon-72-hdpi.png" platform="android" qualifier="hdpi" />
<icon src="res/android/icon-96-xhdpi.png" platform="android" qualifier="xhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
